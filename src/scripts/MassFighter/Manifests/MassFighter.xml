<manifest>
    <main-class>scripts.MassFighter.MassFighter</main-class>
    <name>MassFighter</name>
    <description>AIO PowerFighter for RS3</description>
    <version>1.0</version>
    <compatibility>
        <game-type>RS3</game-type>
    </compatibility>
    <categories>
        <category>COMBAT</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>Ozzy-MassFighter</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden>
    <access>public</access>
    <tags>
        <tag>Combat</tag>
        <tag>Fighter</tag>
        <tag>AIO</tag>
        <tag>PowerFighter</tag>
    </tags>
</manifest>