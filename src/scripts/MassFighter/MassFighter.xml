<manifest>
    <main-class>scripts.MassFighter.MassFighter</main-class>
    <name>MassFighter</name>
    <description>AIO Fighter for RS3 and OSRS.</description>
    <version>1.8.6</version>
    <compatibility>
        <game-type>OSRS</game-type>
        <game-type>RS3</game-type>
    </compatibility>
    <categories>
        <category>COMBAT</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>Ozzy-MassFighter</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden>
    <access>public</access>
    <tags>
        <tag>Combat</tag>
        <tag>Fighter</tag>
        <tag>AIO</tag>
        <tag>PowerFighter</tag>
        <tag>Banking</tag>
        <tag>Moneymaking</tag>
    </tags>
    <resources>
        <resource>scripts/MassFighter/GUI/FighterGUI.fxml</resource>
    </resources>
</manifest>